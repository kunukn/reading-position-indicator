{
  "name": "reading-position-indicator",
  "version": "2.0.3",
  "description": "Reading position indicator on a webpage",
  "main": "dist/rpi.bundle.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --progress --config webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --env.production",
    "build": "cross-env NODE_ENV=production webpack --env.production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/reading-position-indicator.git"
  },
  "keywords": [
    "reading position indicator"
  ],
  "author": "kunukn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunukn/reading-position-indicator/issues"
  },
  "homepage": "https://github.com/kunukn/reading-position-indicator#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "*",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
